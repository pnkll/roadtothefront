\n}\n\nexport default Profile","/home/pnkll/Documents/pp/roadtothefront/src/components/Settings/Settings.jsx",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/News/News.jsx",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/Dialogs/Message/Message.jsx",["166"],"import classes from './Message.module.css'\n\nfunction Message(props) {\n\n  return (\n    <div className={classes.message}><img src={props.avatar}/>{props.text}</div>\n  )\n}\n\nexport default Message","/home/pnkll/Documents/pp/roadtothefront/src/components/Dialogs/DialogItem/DialogItem.jsx",["167"],"import classes from './DialogItem.module.css'\nimport { NavLink } from 'react-router-dom'\n\nfunction DialogItem(props) {\n  return (\n    <div className={classes.dialog}><NavLink to={props.path}><img src={props.avatar}/>{props.name}</NavLink></div>\n  )\n}\n\nexport default DialogItem","/home/pnkll/Documents/pp/roadtothefront/src/components/Nav/SidebarFriends/SidebarFriends.jsx",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/Profile/ProfileInfo/ProfileInfo.jsx",["168","169"],"import { NavLink } from 'react-router-dom'\nimport classes from './ProfileInfo.module.css'\nimport ProfileStatus from './ProfileStatus'\n\nfunction ProfileInfo(props) {\n    return (\n        <div>\n            {/* <div className={classes.ava}><img src={props.user.photos.large != null\n                ? props.user.photos.large\n                : 'https://cdn1.ozone.ru/s3/multimedia-a/c1200/6064056070.jpg'} /></div>\n            <div className={classes.fullname}>{props.user.fullName}</div> */}\n            <div>Status: <ProfileStatus /></div>\n            {/* <div>Socials: <a href={`https://${props.user.contacts.github}`}>GitHub</a></div> */}\n\n        </div>\n    )\n}\n\nexport default ProfileInfo","/home/pnkll/Documents/pp/roadtothefront/src/components/Profile/My posts/MyPosts.jsx",["170"],"import React from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { updatePostText, addPost } from '../../../redux/profileReducer';\nimport classes from './MyPosts.module.css'\nimport Post from './Post/Post';\n\nfunction MyPosts(props) {\n\n    let newPostElem = React.createRef();\n    let postElems = props.profilePage.posts.map(p => (<Post key={p.id} message={p.message} likesCount={p.likesCount} avatar={p.avatar} />));\n\n    const dispatch = useDispatch()\n\n    const onUpdatePostText = () => {\n        let newText = newPostElem.current.value;\n        dispatch(updatePostText(newText))\n    }\n\n    const onAddPost = () => {\n        dispatch(addPost())\n    }\n\n    return (\n        <div className={classes.posts}>\n            My posts\n            <div><textarea onChange={onUpdatePostText} ref={newPostElem} value={props.profilePage.newPostText} /></div>\n            <button onClick={onAddPost}>Add post</button>\n            {postElems}\n        </div>\n    )\n}\n\nexport default MyPosts\n","/home/pnkll/Documents/pp/roadtothefront/src/components/Nav/SidebarFriends/FriendsItem/FriendItem.jsx",["171"],"import classes from './FriendItem.module.css'\nimport {NavLink} from 'react-router-dom'\n\nfunction FriendItem(props) {\n    return (\n        <div className={classes.friend}>\n            <NavLink to={`profile/${props.users.path}`}>\n            <img src={props.users.avatar} />\n            {props.users.name}\n            </NavLink>\n        </div>\n    )\n}\n\nexport default FriendItem","/home/pnkll/Documents/pp/roadtothefront/src/components/Profile/My posts/Post/Post.jsx",["172"],"import classes from './Post.module.css'\n\nfunction Post(props) {\n\n    return (\n        <div className={classes.item}>\n            <img src={props.avatar} />\n            <div className={classes.text}>{props.message}</div>\n            <div className={classes.likes}>Liked: {props.likesCount}</div>\n        </div>\n    )\n}\n\nexport default Post","/home/pnkll/Documents/pp/roadtothefront/src/redux/usersReducer.js",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/tests/clock/clock.jsx",[],["173","174"],"/home/pnkll/Documents/pp/roadtothefront/src/components/Users/Users.jsx",["175","176","177"],"import React, { useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { NavLink } from \"react-router-dom\";\nimport classes from './Users.module.css';\nimport Preloader from \"../default/Preloader/Preloader\";\nimport { followThunk, getUsersThunk, onSetPageThunk, unFollowThunk } from \"../../redux/async/usersThunk\";\n\nlet Users = (props) => {\n\n    const dispatch = useDispatch()\n    const pageCount = useSelector(state => state.usersPage.pageCount)\n    const pageSize = useSelector(state => state.usersPage.pageSize)\n    const currentPage = useSelector(state => state.usersPage.currentPage)\n    const users = useSelector(state => state.usersPage.users)\n    const isFetching = useSelector(state => state.usersPage.isFetching)\n    const button = useSelector(state => state.usersPage.button)\n\n    useEffect(() => {\n        if (users.length === 0) {\n            dispatch(getUsersThunk(currentPage, pageSize))\n        }\n    }, [])\n\n    let header = 'Users'\n    let but = <button>show more</button>\n    let pages = []\n\n    for (let i = 1; i <= Math.ceil(pageCount / pageSize); i++) {\n        pages.push(i)\n    }\n\n    return (\n\n        <div>{header} {\n            users.map(u => (\n                <div key={u.id} className={classes.users}>\n                    <div className={classes.user}>\n                        <NavLink to={`../profile/${u.id}`}><img src={u.photos.small != null\n                            ? u.photos.small\n                            : \"https://cdn1.ozone.ru/s3/multimedia-a/c1200/6064056070.jpg\"} /></NavLink><br />\n                        <div>\n                            {u.followed\n                                ? <button className={classes.followed} onClick={() => {\n                                    dispatch(unFollowThunk(u.id))\n\n                                }}>unfollow</button>\n                                : <button disabled = {button} className={classes.followed} onClick={() => {\n                                    dispatch(followThunk(u.id))\n                                }}>follow</button>}\n                        </div>\n                    </div>\n\n                    <div className={classes.info}>\n                        <div className={classes.name}>\n                            <div>{u.name}</div>\n                            <div>{u.status != null\n                                ? u.status\n                                : 'Here need a status'}</div>\n                        </div>\n                        <div className={classes.location}>\n                            <div>{\"Atlantic ocean\"}</div>\n                            <div>{\"Bikini Bottom\"}</div>\n                        </div>\n                    </div>\n                </div>))\n\n        }\n            {/* {but} */}\n            <div className={classes.btns}>\n                <div>{isFetching ? <Preloader /> : null}</div>\n                {pages.map(p => {\n                    return <span onClick=\n                        {(e) => { dispatch(onSetPageThunk(p, pageSize)) }} className={currentPage === p\n                            ? classes.selectedPage\n                            : classes.page}>{p}</span>\n                })}\n            </div>\n\n        </div>\n    )\n}\n\nexport default Users","/home/pnkll/Documents/pp/roadtothefront/src/App.js",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/default/Preloader/Preloader.jsx",["178"],"import React from \"react\"\n\nconst Preloader = _ => {\n    return <><img src='https://klawood.com.ua/images/default/uploads.gif' /></>\n}\n\nexport default Preloader","/home/pnkll/Documents/pp/roadtothefront/src/redux/authReducer.js",[],"/home/pnkll/Documents/pp/roadtothefront/src/components/api/api.js",[],"/home/pnkll/Documents/pp/roadtothefront/src/redux/async/profileThunk.